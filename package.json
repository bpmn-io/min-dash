{
  "name": "min-dash",
  "version": "3.4.0",
  "description": "Minimum utility toolbelt",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "all": "run-s lint test distro",
    "bundle": "rollup -c",
    "copy": "cpx -v lib/*.d.ts dist",
    "distro": "run-s copy bundle test:integration",
    "dev": "npm test -- --watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s distro",
    "test": "mocha -r @babel/register -r source-map-support/register --full-trace test/*.spec.js",
    "test:integration": "mocha --full-trace test/integration/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/min-dash.git"
  },
  "keywords": [
    "lodash",
    "utility",
    "tool",
    "belt"
  ],
  "author": "bpmn.io <http://bpmn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpmn-io/min-dash/issues"
  },
  "sideEffects": false,
  "homepage": "https://github.com/bpmn-io/min-dash",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.1.2",
    "cpx": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-plugin-bpmn-io": "^0.6.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.1",
    "rollup": "^0.66.5",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-uglify": "^6.0.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "source-map-support": "^0.5.0"
  }
}
